#include <stdio.h>

int main() {

    double preco1, preco2, total;
    int num1, cod1, num2, cod2;

    scanf("%d", &cod1);
    scanf("%d", &num1);
    scanf("%lf", &preco1);
    scanf("%d", &cod2);
    scanf("%d", &num2);
    scanf("%lf", &preco2);

    total = (num1 * preco1) + (num2 * preco2);

    printf("VALOR A PAGAR: R$ %.2lf\n", total);

    return 0;
}
